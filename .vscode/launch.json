        "version": "0.2.0",
        "configurations": [
            
            
          {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/FREEARM",
            "miDebuggerServerAddress": "192.168.0.102:2011",
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "args": ["--target extended-remote 192.168.0.102:2011"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "MIMode": "gdb",
            "externalConsole": false,
            "miDebuggerArgs": "gdb-multiarch",
            "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true,
            }

            ]
        }
        ]
      
}